<Window x:Class="TeamsLeague.UI.WPF.Views.Windows.TeamEvents.SoloRankWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeamsLeague.UI.WPF.Views.Windows.TeamEvents"
        mc:Ignorable="d"
        WindowStyle="None" WindowStartupLocation="CenterScreen"
        MinWidth="1240" MinHeight="736" Background="#00000000"
        Title="SoloRankWindow">
    <Border Background="#FF3D7E88">
        <Grid>
            <Frame x:Name="Event_Frame" Panel.ZIndex="0"/>
            <Label x:Name="EventStadiaName" Panel.ZIndex="1" Content="Preview" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5" Width="220" Height="120" FontFamily="Arial Black" FontSize="36" FontWeight="Bold" Background="#FF255663" Foreground="CadetBlue" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
            <Button x:Name="Back_Button" Panel.ZIndex="1" Content="BACK" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="5" Width="180" Height="120" FontFamily="Arial Black" FontSize="36" FontWeight="Bold" Background="#FF255663" Foreground="CadetBlue" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Click="Back_Button_Click"/>
        </Grid>
    </Border>
</Window>
